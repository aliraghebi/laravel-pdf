{
    "name": "arsamme/laravel-mpdf",
    "description": "Laravel Mpdf: Using Mpdf in Laravel to generate Pdfs.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "mpdf/mpdf": "^8.0|^8.1|^8.2"
    },
    "autoload": {
        "psr-4": {
            "ArsamMe\\LaravelPdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArsamMe\\LaravelPdf\\LaravelPdfServiceProvider"
            ],
            "aliases": {
                "PDF": "ArsamMe\\LaravelPdf\\Facades\\LaravelPdf",
                "LaravelPdf": "ArsamMe\\LaravelPdf\\Facades\\LaravelPdf"
            }
        }
    }
}
